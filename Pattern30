/*
5 4 3 2 1
4 3 2 1
3 2 1
2 1
1

*/


import java.util.Scanner;
     class Pattern30
	 {
		 public static void main(String[] args)
		 {
			 Scanner sc=new Scanner(System.in);
			 System.out.println("Enter the number the terms...");
			 int n=sc.nextInt();
			 
			 pattern(n);
		 }
		 
		 
		 static void pattern(int n)
		 {
			
			 for(int i=n;i>=1;i--)
			 {
				 for(int j=i;j>=1;j--)
				 {
					 System.out.print(j+" ");
					
				 }
				 System.out.println();
			 }
		 }
	 }
